   "id": "ba5caa8a",
   "metadata": {},
   "outputs": [],
   "source": [
    "class ostream:\n",
    "    def __lshift__(self,other):\n",
    "        if other==\"endl\":\n",
    "            print()\n",
    "            return self\n",
    "        print(other,end=\"\")\n",
    "        return self\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "27514e8b",
   "metadata": {},
   "outputs": [],
   "source": [
    "cout = ostream()\n",
    "endl=\"endl\""
   ]
